<header class="navbar navbar-fixed-top navbar-inverse">
  <div><%= link_to 'home', root_url, id: "logo"%></div>
  <nav>
    <ul class='nav navbar-nav navbar-right'>
      <% if admin_user? %>
        <li><%= link_to 'users', users_url %></li>
      <% end %>
      <% if logged_in? %>
        <li><%= link_to 'account', user_url(id: current_user.id ) %></li>
        <li><%= link_to 'logout', logout_url, method: :delete %></li>
      <% else %>
        <li><%= link_to 'login', login_url %></li>
        <li><%= link_to 'sign up', signup_url %></li>
      <% end %>
    </ul>
  </nav>
</header>
